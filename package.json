{
    "name": "onda-types",
    "version": "1.1.0",
    "description": "Types repository for Onda applications",
    "main": "index.js",
    "types": "index.d.ts",
    "exports": {
        "./backend-wave": {
            "types": "./backend-wave/index.d.ts",
            "import": "./backend-wave/index.js",
            "require": "./backend-wave/index.js"
        },
        "./backend-onda": {
            "types": "./backend-onda/index.d.ts",
            "import": "./backend-onda/index.js",
            "require": "./backend-onda/index.js"
        },
        "./frontend-app": {
            "types": "./frontend-app/index.d.ts",
            "import": "./frontend-app/index.js",
            "require": "./frontend-app/index.js"
        }
    },
    "files": [
        "backend-wave/**/*",
        "backend-onda/**/*",
        "frontend-app/**/*",
        "index.d.ts",
        "index.js"
    ],
    "scripts": {
        "build": "tsc",
        "prepublishOnly": "npm run build",
        "test": "echo \"No tests specified\" && exit 0"
    },
    "keywords": [
        "types",
        "typescript",
        "onda"
    ],
    "dependencies": {
        "hono": "^4.7.8",
        "zod": "^3.24.4"
    },
    "author": "Your Name",
    "license": "MIT",
    "devDependencies": {
        "typescript": "^5.0.0",
        "@types/node": "^20.0.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/yourusername/onda-types.git"
    }
}
