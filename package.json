{
  "name": "onda-types",
  "version": "1.7.0",
  "description": "Types repository for Onda applications",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "restricted"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./backend-worker-financeiro": {
      "import": "./dist/backend-worker-financeiro/index.js",
      "types": "./dist/backend-worker-financeiro/index.d.ts"
